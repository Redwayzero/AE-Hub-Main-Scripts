local gamePlaceIds = {
    4042427666,   -- AFS
    11545598432   -- AFSX
}

local currentPlaceId = game.PlaceId

local function loadScript(url)
    local loadstringScript = game:HttpGet(url)
    if loadstringScript then
        loadstring(loadstringScript)()
    else
        warn("Failed to fetch script from URL: " .. url)
    end
end

if table.find(gamePlaceIds, currentPlaceId) then
    if currentPlaceId == 4042427666 then
        local loadstringURL = "https://raw.githubusercontent.com/Redwayzero/AE-AFS-/main/AE%20Afs"
        loadScript(loadstringURL)
    elseif currentPlaceId == 11545598432 then
        local loadstringURL = "https://raw.githubusercontent.com/Redwayzero/AE-Afsx-/main/AE%20Afsx"
        loadScript(loadstringURL)
    end
    -- Add more elseif blocks for other game PlaceIds here
else
    local loadstringURL = "https://pastebin.com/raw/NiEmSuNH"
    loadScript(loadstringURL)
end
